#include <bits/stdc++.h>
using namespace std;
using ll = long long;
using ull = unsigned long long;
 
#define testcase() {ll t; cin >> t; while(t--) {solve();}}
#define all(x) (x).begin(), (x).end()
#define sz(x) ((ll)(x).size())
#define pb push_back
#define ar array
#define bug() cout << "######PLEASE_NO_BUG######\n"
 
void solve() {
	int n, sa=0, sb=0, sc=0;
	cin >> n;
	for(int i=0,a,b,c; i<n; ++i) {
		cin >> a >> b >> c;
		sa+=a;
		sb+=b;
		sc+=c;
	}
	if(sa||sb||sc) cout << "NO";
	else cout << "YES";
}
 
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
//	testcase();
	solve();
	return 0;
}
